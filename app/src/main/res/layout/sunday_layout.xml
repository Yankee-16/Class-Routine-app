<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingVertical="8sp" >


    <androidx.cardview.widget.CardView
        android:layout_height="150sp"
        android:layout_width="match_parent"
        app:cardElevation="10dp"
        app:cardCornerRadius="12dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        android:foreground="?android:attr/selectableItemBackground"
        android:clickable="true"
        >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp" >

            <TextView
                android:id="@+id/title4"
                android:padding="3dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Wireless Programming"
                android:textColor="@color/black"
                android:textSize="@dimen/_15sdp"
                />

            <TextView
                android:id="@+id/code4"
                android:layout_below="@id/title4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CSE 334"
                android:textSize="15dp"
                android:padding="3dp"
                />

            <TextView
                android:id="@+id/initial_label4"
                android:layout_below="@+id/code4"
                android:layout_width="@dimen/_140sdp"
                android:layout_height="wrap_content"
                android:text="Teacher Initial"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                android:paddingLeft="3dp"
                />

            <TextView
                android:id="@+id/section_label4"
                android:layout_below="@id/code4"
                android:layout_toRightOf="@id/initial_label4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Section"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                android:paddingLeft="3dp"
                />

            <TextView
                android:id="@+id/initial4"
                android:layout_below="@id/initial_label4"
                android:layout_width="@dimen/_140sdp"
                android:layout_height="wrap_content"
                android:text="MJM"
                android:textSize="@dimen/_12sdp"
                android:paddingLeft="3dp"
                />

            <TextView
                android:id="@+id/section4"
                android:layout_below="@id/section_label4"
                android:layout_toRightOf="@id/initial4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="O-12"
                android:textSize="@dimen/_12sdp"
                android:paddingLeft="3dp"
                />

            <ImageView
                android:id="@+id/timeImage4"
                android:layout_below="@id/initial4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/time"
                android:padding="3dp"
                />

            <TextView
                android:id="@+id/time4"
                android:layout_below="@+id/initial4"
                android:layout_toRightOf="@id/timeImage4"
                android:layout_width="@dimen/_120sdp"
                android:layout_height="wrap_content"
                android:text="10:00 AM - 11:30 AM"
                android:textSize="@dimen/_12sdp"
                android:textColor="@color/black"
                android:padding="3dp"/>

            <ImageView
                android:id="@+id/roomImage4"
                android:layout_below="@id/section4"
                android:layout_toRightOf="@id/time4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/location"
                android:padding="3dp"
                />

            <TextView
                android:id="@+id/room4"
                android:layout_below="@id/section4"
                android:layout_toRightOf="@id/roomImage4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="105 DT"
                android:textColor="@color/black"
                android:padding="3dp" />

        </RelativeLayout>
    </androidx.cardview.widget.CardView>

